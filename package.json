{
  "name": "eyesoffcr",
  "version": "1.0.0",
  "description": "EyesOffCR.org - Protecting Privacy in Cedar Rapids",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:e2e && npm run test:a11y && npm run test:html",
    "test:e2e": "xvfb-run -a cypress run",
    "test:a11y": "pa11y-ci ./index.html ./about.html ./media_package.html",
    "test:html": "html-validate *.html",
    "start": "NODE_ENV=production node server.js",
    "serve": "npm run build && npm start"
  },
  "dependencies": {
    "compression": "^1.8.0",
    "express": "^4.21.2",
    "helmet": "^7.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.4",
    "cypress-axe": "^1.5.0",
    "html-validate": "^8.8.0",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4"
  }
}
